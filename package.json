    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "kind-of": ">=6.0.3",
    "node-forge": ">=0.10.0",
    "yargs-parser": ">=13.1.2",
    "serialize-javascript": ">=3.1.0"
  },
  "dependencies": {
    "apisauce": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "leaflet": "^1.5.1",
